<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>UserLogs</title>
    <link rel="stylesheet" type="text/css" href="userlogs.css">
  </head>
  <body>
    <header>
      <div class = "main">
        <div class="logo">
          <a href="admin.html">
            <img src="srm.png">
          </a>
        </div>
        <u1>
          <li><a href="adminlogin.html">Log Out</a></li>
        </u1>
        <div class="getbut">
              <a href="/userlist" class="btn">GET DATA</a>
          </section>
          </div>
        </div>
        <ul>
          <%
            var list = '';
            for (i = 0; i < userlogs.length; i++) {
              list += '<li>' + "       UserID      "+userlogs[i].User + "       Time      " +  userlogs[i].Time +"       Machine Used     "+userlogs[i].MachineUsed + "       Date     " +  userlogs[i].Date  +'</li>';
            }
          %>
          <%- list %>
       </ul>
      
      <div class="promotion">
        <h5><center>Developed by The Project Team</center></h5>
      </div>
    </header>
  </body>
</html>
